generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @db.Uuid @default(uuid())
  name String
  email String @unique
  age Int?
  isActive Boolean @default(true) @map("is_active") 

  @@map("users")
  @@index([name])
}